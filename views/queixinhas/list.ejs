<% include ./../_layouts/top %>
    <div class="row">
            <div class="box">
                <div class="col-lg-12">

                    <h1>Lista de Queixinhas</h1>
                    <ul>

                    <% queixinhas.forEach(function(queixinha) { %>

                        <li id="queixinha-<%=queixinha.id%>" class="queixinha" >
                            <b>
                                <a href="/queixinhas/queixinha/<%=queixinha.id%>"><%= queixinha.title %></a>
                            </b><br>
                            <small><%= queixinha.owner %></small>
                        </li>
                    <% }) %>
                    </ul>
                </div>
            </div>
        </div>

    <!-- /.container -->
<% include ./../_layouts/footer %>
