<% include ./../_layouts/top %>
<% include ./top_of_dashboard %>


	<div class="box" style="color:green">
		<h1>My Queixinhas</h1>
	</div>


<% myqueixinhas.forEach(function(queixinha) { %>


	<div class="box">
		<div class="col-lg-12">
        <% if(queixinha.state) {%>
			<div class="col-lg-7">
            	<i class="fa fa-keyboard-o fa-2x"></i>
            	<a href="/queixinhas/<%=queixinha.id%>"><%= queixinha.title %></a>
        	</div>
        	<div class="col-lg-2">
            	<a href=""><small> lock</small></a>
            </div>
        <% }else {%>
        	<div class="col-lg-7">
            	<i class="fa fa-key fa-2x"></i>
            	<a href="/queixinhas/<%=queixinha.id%>"><%= queixinha.title %></a>
            </div>
            <div class="col-lg-2">
            	<a href=""><small> unlock</small></a>
            </div>
        <% }%>

        </div>
    </div>

<% }) %>


<% include ./../_layouts/footer %>